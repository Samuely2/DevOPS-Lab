{
    "version": 2,
    "builds": [
      {
        "src": "api/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/swagger",
        "dest": "/static/swagger-ui.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.py"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }
  