{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "public/swagger-ui.html", "use": "@vercel/static" },
      { "src": "public/swagger.json", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/public/swagger-ui.html" }, 
      { "src": "/swagger.json", "dest": "/public/swagger.json" },
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ]
  }
  