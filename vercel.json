{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/swagger-ui", "dest": "/public/swagger-ui.html" },
      { "src": "/swagger.json", "dest": "/public/swagger.json" },
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ]
  }
  